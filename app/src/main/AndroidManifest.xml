<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dalongtech.testapplication">

    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>

    <application
        android:allowBackup="true"
        android:name=".app.App"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.dalongtech.testapplication.activity.CustomKayboardActivity"
            android:screenOrientation="landscape"/>
        <activity android:name="com.dalongtech.testapplication.activity.VoicePlayActivity" />
        <activity
            android:name="com.dalongtech.testapplication.activity.BigImageActivity"
            android:theme="@style/myTransparent" />
        <activity android:name="com.dalongtech.testapplication.activity.ShortcutBadgeActivity" />
        <activity android:name="com.dalongtech.testapplication.activity.TestFragmentActivity" />
        <activity
            android:name="com.dalongtech.testapplication.activity.SlideClosePictureActivity"
            android:theme="@style/myTransparent" />
        <activity android:name="com.dalongtech.testapplication.activity.CustomProgressBarActivity" />
        <activity android:name="com.dalongtech.testapplication.activity.BTRecyclerActivity" />
        <activity android:name="com.dalongtech.testapplication.activity.SmallPinnedHeaderActivity" />
        <activity android:name="com.dalongtech.testapplication.activity.FlexibleLayoutActivity" />
        <activity android:name="com.dalongtech.testapplication.activity.GetAddressListAndPermissionActivity" />
        <activity android:name="com.dalongtech.testapplication.activity.RecyclerDivideActivity" />
        <activity android:name="com.dalongtech.testapplication.activity.ImageViewTouchActivity" />
        <activity android:name="com.dalongtech.testapplication.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.dalongtech.testapplication.activity.ScrollConflictActivity" />
        <activity android:name=".activity.AnalysisDemoActivity" />
        <activity android:name=".activity.ActionQueueActivity" />


        <!--        角标设置服务-->
        <service
            android:name="com.dalongtech.testapplication.component.BadgeIntentService"
            android:exported="false" />

        <meta-data
            android:name="ANALYSYS_AUTO_COLLECTION_MAC"
            android:value="true"/>
        <meta-data
            android:name="ANALYSYS_AUTO_COLLECTION_IMEI"
            android:value="true"/>
    </application>

</manifest>